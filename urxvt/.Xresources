URxvt.perl-ext-common: font-size,url-select,selection-to-clipboard,keyboard-select

! urxvt general settings
URxvt.cursor:               #a8cdc1
URxvt.scrollBar:            false
URxvt.font:                 xft:Cascadia Code:pixelsize=22
URxvt.iso14755:             false
URxvt.iso14755_52:          false
URxvt.saveLines:            15000
URxvt.internalBorder:       0
URxvt.letterSpace:          -1
URxvt.url-select.launcher:  qutebrowser
URxvt.url-select.underline: true

! urxvt keybinds
URxvt.keysym.Control-Escape: perl:keyboard-select:activate
URxvt.keysym.M-u:            perl:url-select:select_next
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

! urxvt colorscheme
#define COLOR_FG                  #dcdccc
#define COLOR_BG                  #3a3a3a
#define COLOR_TERM_BLACK          COLOR_BG
#define COLOR_TERM_DARK_RED       #705050
#define COLOR_TERM_DARK_GREEN     #304a3d
#define COLOR_TERM_DARK_YELLOW    #dfaf8f
#define COLOR_TERM_DARK_BLUE      #506070
#define COLOR_TERM_DARK_MAGENTA   #dc8cc3
#define COLOR_TERM_DARK_CYAN      #8cd0d3
#define COLOR_TERM_LIGHT_GRAY     COLOR_FG
#define COLOR_TERM_DARK_GRAY      #709080
#define COLOR_TERM_LIGHT_RED      #dca3a3
#define COLOR_TERM_LIGHT_GREEN    #c3bf9f
#define COLOR_TERM_LIGHT_YELLOW   #f0dfaf
#define COLOR_TERM_LIGHT_BLUE     #94bff3
#define COLOR_TERM_LIGHT_MAGENTA  #ec93d3
#define COLOR_TERM_LIGHT_CYAN     #93e0e3
#define COLOR_TERM_WHITE          #ffffff

URxvt.background: COLOR_BG
URxvt.foreground: COLOR_FG
URxvt.color0:     COLOR_TERM_BLACK
URxvt.color1:     COLOR_TERM_DARK_RED
URxvt.color2:     COLOR_TERM_DARK_GREEN
URxvt.color3:     COLOR_TERM_DARK_YELLOW
URxvt.color4:     COLOR_TERM_DARK_BLUE
URxvt.color5:     COLOR_TERM_DARK_MAGENTA
URxvt.color6:     COLOR_TERM_DARK_CYAN
URxvt.color7:     COLOR_TERM_DARK_GRAY
URxvt.color8:     COLOR_TERM_DARK_GRAY
URxvt.color9:     COLOR_TERM_LIGHT_RED
URxvt.color10:    COLOR_TERM_LIGHT_GREEN
URxvt.color11:    COLOR_TERM_LIGHT_YELLOW
URxvt.color12:    COLOR_TERM_LIGHT_BLUE
URxvt.color13:    COLOR_TERM_LIGHT_MAGENTA
URxvt.color14:    COLOR_TERM_LIGHT_CYAN
URxvt.color15:    COLOR_TERM_WHITE
